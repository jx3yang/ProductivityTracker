type Card {
  _id: ID!
  name: String!
  dueDate: String
  parentBoardId: ID!
  parentListId: ID!
}

type CardMetaData {
  _id: ID!
  name: String!
  dueDate: String
}

input NewCard {
  name: String!
  dueDate: String
  parentBoardId: ID!
  parentListId: ID!
}

input ChangeCardOrder {
  boardId: ID!
  srcListId: ID!
  destListId: ID!
  cardId: ID!
  srcIdx: Int!
  destIdx: Int!
}
